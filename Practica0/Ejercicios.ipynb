    "from math import gcd\n",
    "\n",
    "def minimo_comun_multiplo(a, b):\n",
    "    return abs(a * b) // gcd(a, b)\n",
    "\n",
    "def maximo_comun_divisor(a, b):\n",
    "    return gcd(a, b)\n"
